{
	"name": "Merge Windows",
	"version": "1.0.1",
	"description": "Merge windows into the active one",
	"homepage_url": "https://github.com/sindresorhus/merge-windows",
	"manifest_version": 2,
	"minimum_chrome_version": "43",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_title": "Merge Windows",
		"default_icon": "icon.png"
	}
}
